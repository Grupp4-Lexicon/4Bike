
@using _4Bike.Areas.Identity.Data
@using Microsoft.AspNetCore.Identity

@model _4Bike.Models.ViewModels.ManufacturerViewModel
@inject UserManager<ApplicationUser> UserManager
@{
    ViewData["Title"] = "AddManufacturer";
}





<div class="row">
    <div class="col-12 col-md-8 col-lg-10">
        <h5>Add Manufacturer</h5>
        <form asp-action="AddManufacturer" method="post">
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value=" Add" class=" btn btn-primary" />
            </div>
        </form>


    </div>

    <div class="col-12 col-md-4 col-lg-2">
    <partial name="/Views/Shared/SidebarPartial.cshtml" model="UserManager.GetUserName(User)" />
    </div>

</div>
    

