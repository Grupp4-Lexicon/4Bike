@model IEnumerable<_4Bike.Areas.Identity.Data.ApplicationUser>

@{
    ViewData["Title"] = "Index";
}

<h1>Manage users</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>First name</th>
            <th>Last name</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.Email</td>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.Address</td>
            <td><a asp-controller="Role" asp-action="EditUser" asp-route-id="@item.Id">Edit</a></td>
            <td>Delete</td>
        </tr>
        }
    </tbody>
</table>